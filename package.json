{
  "name": "ewa-website",
  "version": "1.0.0",
  "description": "Eastlake Wolfpack Association Website",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "standardize:zelle": "node scripts/standardize-zelle.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
